{
    "name": "carestad/laravel-caffeine",
    "description": "Keeping Your Laravel Forms Awake",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jenssegers/model",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/Kyzegs/model.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        }
    ],
    "require": {
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "jenssegers/model": "dev-master@dev"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^8.0",
        "orchestra/testbench-dusk": "^8.0",
        "orchestra/testbench": "^8.0",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneaLabs\\LaravelCaffeine\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelCaffeine\\Providers\\Service"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
